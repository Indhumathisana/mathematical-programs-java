import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = a%9;
int c=a/9;
if(a%9!=0){
System.out.print(b);}
        for(int i=0;i<c;i++){
            System.out.print("9");
        }
    }
}